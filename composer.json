{
  "name": "photogabble/tuppence",
  "description": "Extremely small framework.",
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Dann",
      "email": "simon.dann@gmail.com"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": "^8.0",
    "league/container": "^4.2",
    "league/event": "^3.0",
    "league/route": "^5.1",
    "laminas/laminas-diactoros": "^3.2",
    "laminas/laminas-httphandlerrunner": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Photogabble\\Tuppence\\": "src",
      "Photogabble\\Tuppence\\Tests\\Helpers\\": "tests/Helpers"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Photogabble\\Tuppence\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.0-dev"
    }
  }
}
